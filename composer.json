{
    "name": "pyaesone17/laravel-pretty-handler",
    "type": "library",
    "description": ":package_description",
    "keywords": [
        "pyaesone17",
        "laravel-pretty-handler"
    ],
    "homepage": "https://github.com/pyaesone17/laravel-pretty-handler",
    "license": "MIT",
    "authors": [
        {
            "name": "Pyae Sone Nyein",
            "email": "promise286@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0||~6.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Pyaesone17\\LaravelPrettyHandler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pyaesone17\\LaravelPrettyHandler\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
